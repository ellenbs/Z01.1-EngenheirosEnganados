function pow 3
    push constant 1
    pop local 0   # resultado

    push argument 1
    pop local 1  # numero de vezes

    label loop
        push local 1
        push constant 0
        eq
        if-goto end

        push argument 0
        push local 0
        call mult 2
        pop local 0

        push local 1 
        push constant 1
        sub
        pop local 1
        goto loop

    label end
        push local 0
        return 
